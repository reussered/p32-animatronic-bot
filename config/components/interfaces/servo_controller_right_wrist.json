{
    "relative_filename":  "config/components/interfaces/servo_controller_right_wrist.json",
    "version":  "1.0.0",
    "author":  "config/author.json",
    "interface_id":  "SERVO_CONTROLLER_RIGHT_WRIST",
    "interface_type":  "DUAL_SERVO_CONTROLLER",
    "description":  "Two-servo controller interface for right wrist 2DOF joint",
    "controller_assignment":  "ESP32_C3_ARM_CONTROLLER_RIGHT",
    "servo_count":  2,
    "control_frequency":  "1000_HZ",
    "servo_assignments":  {
                              "wrist_flex_extend":  {
                                                        "servo_type":  "MG996R_SERVO",
                                                        "pwm_pin":  "GPIO_12",
                                                        "feedback_pin":  "GPIO_A1"
                                                    },
                              "wrist_radial_ulnar_deviation":  {
                                                                   "servo_type":  "DS3218_SERVO",
                                                                   "pwm_pin":  "GPIO_13",
                                                                   "feedback_pin":  "GPIO_A2"
                                                               }
                          },
    "safety_configuration":  {
                                 "emergency_stop_pin":  "GPIO_0",
                                 "precision_mode":  "HIGH_RESOLUTION_CONTROL_AVAILABLE",
                                 "position_limits":  "SOFTWARE_LIMITS_ACTIVE"
                             }
}
