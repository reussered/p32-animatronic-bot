#ifndef DYNAMIC_GPIO_DRIVER_HDR
#define DYNAMIC_GPIO_DRIVER_HDR

#include <esp_err.h>
#include <stdint.h>

/**
 * @brief Initialize dynamic_gpio_driver component
 * @return ESP_OK on success, error code otherwise
 */
esp_err_t dynamic_gpio_driver_init(void);

/**
 * @brief Execute dynamic_gpio_driver component action
 * Called periodically by subsystem dispatcher
 */
void dynamic_gpio_driver_act(void);

#endif // DYNAMIC_GPIO_DRIVER_HDR
